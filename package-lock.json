{
  "name": "micro-frontend-root",
  "private": true,
  "workspaces": [
    "microfrontend-store-home",
    "microfrontend-store-cart",
    "container",
    "backend"
  ],
  "scripts": {
    "dev:home": "cd microfrontend-store-home && npm run dev",
    "dev:cart": "cd microfrontend-store-cart && npm run dev",
    "dev:all": "concurrently \"npm run dev:home\" \"npm run dev:cart\""
  },
  "devDependencies": {
    "concurrently": "^8.0.0"
  }
}